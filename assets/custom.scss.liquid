// GENERAL
  .warning.animated.bounceIn {
    top: 0px;
    height: 30px;
    width: 100%;
    background: aliceblue;
    position: fixed;
    z-index: 9999;
    left: 0;
  }

  .flex {
    display: flex !important;
  }
  .mobile {
    display: none;
  }
  .desktop {
    display: initial;
  }
  @media screen and (max-width: 768px) {
    .mobile-flex {
      display: flex !important;
    }
    .mobile {
      display: initial;
    }
    .desktop {
      display: none;
    }
  }

  .container {
    max-width: 1000px;
  }

  .div-centrage {
    width: 100%;
    position: relative;
    height: 100px;
    .div-centre {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
    }
  }



  h1,
  .h1,
  h1.title,
  h2,
  .h2,
  h2.title,
  .collection_title,
  h1.headline {
    font-family: 'HTNeonRegular' !important;
    font-size: 2.2rem;
    letter-spacing: 1px;
    text-transform: lowercase !important;

    @media only screen and (max-width: 640px) {
      font-size: 1.4rem;
      line-height: 1.2em;
    }

    &::first-letter {
      text-transform: uppercase;
    }
  }

// BUTTONS
  .action_button, a.action_button {
    background: $vert_principal !important;
    color: #fff !important;
    -webkit-appearance: none !important;
    border-radius: 3px !important;
    /*border: 2px solid #fff !important;*/
    border-left: none !important;
    // margin-left: -4px !important;
    cursor: pointer !important;
    border: 0px solid #ffffff !important;
    text-transform: none !important;
    letter-spacing: 0 !important;
  }


  .top_bar .icon-down-arrow {
    display: none;
  }

// PRODUCT GRID

  .bio-image {
    background-image: url("{{ settings.bio_image | img_url }}");
    background-size: 60px 80px;
    background-size: contain;
    background-repeat: no-repeat;
    position: absolute;
    z-index: 10;
    width: 100px;
    height: 80px;
      background-color: transparent;

    top:10px;
    left:10px;
  }

  .product-wrap {

    .bio-image {
      background-image: url("{{ settings.bio_image | img_url }}");
      background-size: contain;
      background-repeat: no-repeat;
      position: absolute;
      z-index: 10;
      width: 50px;
      height: 40px;
      background-color: transparent;
      top:10px;
      left:10px;


    }



    .thumbnail img {
      max-height: 200px;
      min-height: 200px;
    }

    .image-element__wrap:after {
      content: '';
