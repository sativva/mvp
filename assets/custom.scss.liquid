// NEWSLETTER

  .newsletter_section {
    position: relative;

    .container {
      max-width: 1200px;

      @media screen and (max-width: 640px) {
        width: 90vw;
      }
    }

    .overlay {
      background-color: rgba(0, 0, 0, .7);
    }

    .section_form {
      display: flex;

      @media screen and (max-width: 640px) {
        flex-direction: column;
      }

      .block-text {
        width: 45vw;
        text-align: right;
        padding-right: 20px;
        z-index: 999;

        @media screen and (max-width: 640px) {
          width: 90vw;
          text-align: center;
          padding-right: 0;
        }

        .title {
          font-family: 'Open sans';
          font-weight: bold;
          font-size: 1rem;
          margin: 0;
        }

        .title,
        .newsletter-text {
          color: #fff;
          letter-spacing: 0px;
          font-size: 15px;

          @media screen and (max-width: 640px) {
            margin-bottom: 5px;
          }

        }
      }

      .newsletter {
        width: 55vw;
        z-index: 999;

        @media screen and (max-width: 640px) {
          width: 90vw;
        }


          .action_button.sign_up {
            background: $alert !important;
          }

      }
    }
  }

