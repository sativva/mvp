// VARIABLES

$vert_principal: {{ settings.vert_principal }};
$alert: {{ settings.alert }};
$grey: #A8A7A9;
$lightgrey: #FAFAFA;
$midgrey: #F5F5F5;
$night-blue: #1F2C42;


/* #Font-Face
================================================== */

  @font-face {
    font-family: 'HTNeonRegular';
    src: url('HTNeonRegular.eot');
    src: url('HTNeonRegular.eot?iefix') format('eot'),
      url('HTNeonRegular.woff') format('woff'),
      url('HTNeonRegular.ttf') format('truetype'),
      url('HTNeonRegular.svg#HTNeonW03-Regular') format('svg');
    font-weight: normal;
    font-style: normal;
     font-display: swap;
  }

  @font-face {
    font-family: 'Rift-Demi';
    src: url('Rift-Demi.eot');
    src: url('Rift-Demi.eot?iefix') format('eot'),
      url('Rift-Demi.woff') format('woff'),
      url('Rift-Demi.ttf') format('truetype'),
      url('Rift-Demi.svg#Rift-Demi') format('svg');
    font-weight: normal;
    font-style: normal;
  font-display: swap;
  }

