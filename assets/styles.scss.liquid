}

.overlay-data {
  opacity: 0;
  visibility: hidden;
  transition: opacity 0.5s;
  visibility: 0s 0.5s;
  transition: opacity 0.5s, visibility 0s 0.5s;
}

.overlay-open {
  opacity: 1;
  visibility: visible;
  transition: opacity 0.5s;
}

